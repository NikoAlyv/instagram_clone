{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "target": "es2022",
    "module": "ES2022",
    "baseUrl": ".",
    "noUncheckedIndexedAccess": true,
    "paths": {
      "*": ["src/*"],
      "components/*": ["src/components/*"],
      "assets/*": ["./src/assets/*"],
      "router/*": ["./src/router/*"],
      "screens/*": ["./src/screens/*"],
      "theme/*": ["./src/theme/*"],
      "constants/*": ["./src/constants/*"],
      "types/*": ["./src/types/*"],
      
    }
  }
}
